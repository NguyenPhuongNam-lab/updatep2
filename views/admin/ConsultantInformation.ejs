<!DOCTYPE html>
<html lang="en">

<%- include('layouts/header'); -%>

    <body id="page-top">

        <!-- Page Wrapper -->
        <div id="wrapper">

            <%- include('layouts/navbar'); -%>

                <!-- Content Wrapper -->
                <div id="content-wrapper" class="d-flex flex-column">

                    <!-- Main Content -->
                    <div id="content">

                        <%- include('layouts/topbar'); -%>

                            <!-- BOdy-->

                            <!-- Begin Page Content -->
                            <div class="container-fluid">
                                <!-- Page Heading -->
                                <h1 class="h3 mb-2 text-gray-800">List Consultants</h1>
                                <!-- DataTales Example -->
                                <div class="card shadow mb-4">
                                    <div class="card-header py-3">
                                        <h6 class="m-0 font-weight-bold text-primary">Data Tables</h6>
                                        <a href="/create-consultant" class="btn btn-primary btn-user">Create Consultants</a>
                                    </div>
                                    <div class="card-body">
                                        <form action="" method="POST">
                                            <div class="form-group">
                                                <label>ID</label>
                                                <input id="id" name="id" class="form-control" value="<%= listc._id %>" disabled/>
                                            </div>
                                            <div class="form-group">
                                                <label>Full name</label>
                                                <input id="fullName" name="fullName" class="form-control" value="<%= listc.fullName %>" />
                                            </div>
                                            <div class="form-group">
                                                <label>Age</label>
                                                <input id="Age" name="Age" class="form-control" value="<%= listc.Age %>" />
                                            </div>
                                            <div class="form-group">
                                                <label>Date of birth</label>
                                                <input type="date" id="dateOfbirth" name="dateOfbirth" class="form-control" value="<%= listc.dateOfbirth %>" />
                                            </div>
                                            <div class="form-group">
                                                <label>Gender</label>
                                                <input id="Gender" name="Gender" class="form-control" value="<%= listc.Gender %>" />
                                            </div>
                                            <div class="form-group">
                                                <label>Professional</label>
                                                <input id="Professional" name="Professional" class="form-control" value="<%= listc.Professional %>" />
                                            </div>
                                            <div class="form-group">
                                                <label>Address</label>
                                                <input id="Address" name="Address" class="form-control" value="<%= listc.Address %>" />
                                            </div>
                                            <div class="form-group">
                                                <label>Phone Number</label>
                                                <input type="tel" id="phoneNumber" name="phoneNumber" class="form-control" value="<%= listc.phoneNumber %>" />
                                            </div>
                                            <div class="form-group">
                                                <label>Email</label>
                                                <input type="email" id="Email" name="Email" class="form-control" value="<%= listc.Email %>" />
                                            </div>
                                            <input id="update-btn" class="btn btn-primary btn-create" type="submit" value="Save">
                                            <a href="/list-consultant" class="btn btn-primary" type="submit">Return</a>
                                        </form>
                                    </div>
                                </div>
                            </div>
                            <!-- end body-->

                    </div>
                    <!-- End of Main Content -->

                    <%- include('layouts/footer'); -%>

                </div>
                <!-- End of Content Wrapper -->

        </div>
        <!-- End of Page Wrapper -->

        <!-- Scroll to Top Button-->
        <a class="scroll-to-top rounded" href="#page-top">
            <i class="fas fa-angle-up"></i>
        </a>

        <!-- Logout Modal-->
        <%- include('layouts/logout'); -%>

            <%- include('layouts/script'); -%>

    </body>

</html>